{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "func-names": "off",
    "no-use-before-define": "off",
    "no-underscore-dangle": "off",
    "no-plusplus": "off"
  }
}
